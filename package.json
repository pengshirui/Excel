{
    "name": "excel",
    "version": "0.0.1",
    "private": true,
    "main": "main.js",
    "scripts": {
        "watch": "webpack --watch --progress --debug",
        "build": "webpack --progress && cp src/excel/index.html dist/index.html && cp src/main.js dist/main.js && cp package.json dist/package.json",
        "start": "electron dist/main.js",
        "package": "electron-packager ./dist excel --arch=x64 --out=out --platform=win32 --overwrite"
    },
    "dependencies": {
        "babel-core": "^6.26.0",
        "babel-loader": "7.1.2",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "electron": "1.7.9",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-scripts": "1.0.17"
    },
    "devDependencies": {
        "babel": "^6.23.0",
        "electron-packager": "^10.1.0",
        "webpack": "^3.10.0"
    }
}